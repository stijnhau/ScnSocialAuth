{
    "name": "socalnick/scn-social-auth",
    "description": "Uses the HybridAuth PHP library to Enable authentication via Google, Facebook, Twitter, Yahoo!, etc for the ZfcUser ZF2 module.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "facebook"
    ],
    "homepage": "https://github.com/SocalNick/ScnSocialAuth",
    "authors": [
        {
            "name": "Nicholas Calugar",
            "email": "njcalugar@gmail.com",
            "homepage": "http://socalnick.github.com/"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "zendframework/zend-authentication": ">=2.0",
        "zendframework/zend-db": ">=2.0",
        "zendframework/zend-eventmanager": ">=2.0",
        "zendframework/zend-http": ">=2.0",
        "zendframework/zend-i18n": ">=2.0",
        "zendframework/zend-loader": ">=2.0",
        "zendframework/zend-log": ">=2.0",
        "zendframework/zend-modulemanager": ">=2.0",
        "zendframework/zend-mvc": ">=2.0",
        "zendframework/zend-serializer": ">=2.0",
        "zendframework/zend-servicemanager": ">=2.0",
        "zendframework/zend-session": ">=2.0",
        "zendframework/zend-stdlib": ">=2.0",
        "zendframework/zend-test": ">=2.0",
        "zendframework/zend-uri": ">=2.0",
        "zendframework/zend-view": ">=2.0",
        "zf-commons/zfc-user": ">=1.4",
        "hybridauth/hybridauth": ">=2.8"
    },
    "require-dev": {
        "mockery/mockery": "0.8.0"
    },
    "autoload": {
        "psr-0": {
            "ScnSocialAuth": "src/",
            "ScnSocialAuthTest": "test/"
        },
        "classmap": [
            "./"
        ]
    }
}
